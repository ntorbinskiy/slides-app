{
  "name": "gatsby-starter-minimal-ts",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby starter written in TypeScript with no plugins and a single welcome page.",
  "author": "Jude Agboola",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start": "gatsby clean && gatsby develop",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit"
  },
  "license": "0BSD",
  "dependencies": {
    "gatsby": "^5.13.2",
    "gatsby-plugin-no-javascript-utils": "^0.13.1",
    "gatsby-plugin-react-svg": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.3.3"
  }
}
